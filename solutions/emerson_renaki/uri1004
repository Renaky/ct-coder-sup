import java.io.IOException;

import java.util.Scanner; 

public class Main {
 
    public static void main(String[] args) throws IOException {
 
            //Variavel = X
            //Variavel = Y
            //PROD = X * Y;
            
            Scanner sc = new Scanner(System.in);
           
            int X = sc.nextInt();
            int Y = sc.nextInt();
            int SOMA = X * Y;
            
            System.out.println("PROD = " + SOMA);
    }
 
}
